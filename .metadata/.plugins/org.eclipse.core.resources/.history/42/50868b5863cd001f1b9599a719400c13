package thread;

public class SingleThread_1 {

	public static void main(String[] args) throws InterruptedException {
		
		for(int num = 0; num < 30; num++) {
			System.out.println("O");
			Thread.sleep(num);
		}
		
		for(int num = 0; num < 30; num++) {
			System.out.println("X");
			Thread.sleep(num);
		}
	}

}
