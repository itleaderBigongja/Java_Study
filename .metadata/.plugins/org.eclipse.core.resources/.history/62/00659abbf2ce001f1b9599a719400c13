package thread_4;

public class ThreadSync_3 {
	
	public static void main(String[] args) {
		Memory mem = new Memory(16);
		Download down = new Download(mem);
		Play play = new Play(mem);
		
		down.start();
		play.start();
	}
}

class Download extends Thread {
	
	@Override
	public void run() {
		
	}
}
