package thread_4;

public class ThreadSync_5 {

	public static void main(String[] args) {
	
		Thread worker_1 = new Thread(new Runnable() {
			
			@Override
			public void run() {
				for(int i = 0; i < 10; i++) {
					try {
						Thread.sleep(1000);
					} catch (Exception e) {
						// TODO: handle exception
					}
					System.out.print(i+"");
				}
			}
		});
		
		Thread worker_2 = new Thread(new Runnable() {
			
			@Override
			public void run() {
				for(int i = 0; i < 20; i++) {
					
				}
			}
		});
	}
}
